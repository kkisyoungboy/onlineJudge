<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>onlineJudge</display-name>
  <welcome-file-list>
    <welcome-file>contest.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
      <error-code>404</error-code>
      <location>/404page.jsp</location>
  </error-page>
  <error-page>
      <error-code>500</error-code>
      <location>/500page.jsp</location>
  </error-page>
  <listener>
    <listener-class>com.hwf.listener.ConfigListener</listener-class>
  </listener>
  <context-param>
    <param-name>account</param-name>
    <param-value>account</param-value>
  </context-param>
  <context-param>
    <param-name>sessionID</param-name>
    <param-value>sessionID</param-value>
  </context-param>
  <context-param>
    <param-name>isAdmin</param-name>
    <param-value>isAdmin</param-value>
  </context-param>
  <context-param>
    <param-name>checkSessionName</param-name>
    <param-value>checkSession</param-value>
  </context-param>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.hwf.servlet.RegisterServlet</servlet-class>
    <init-param>
      <param-name>status</param-name>
      <param-value>status</param-value>
    </init-param>
    <init-param>
      <param-name>fail</param-name>
      <param-value>fail</param-value>
    </init-param>
    <init-param>
      <param-name>same</param-name>
      <param-value>same</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.hwf.servlet.LoginServlet</servlet-class>
    <init-param>
      <param-name>autoLoginName</param-name>
      <param-value>autoLoginName</param-value>
    </init-param>
    <init-param>
      <param-name>autoLoginSId</param-name>
      <param-value>autoLoginSId</param-value>
    </init-param>
    <init-param>
      <param-name>saveName</param-name>
      <param-value>saveName</param-value>
    </init-param>
    <init-param>
      <param-name>success</param-name>
      <param-value>success</param-value>
    </init-param>
    <init-param>
      <param-name>fail</param-name>
      <param-value>fail</param-value>
    </init-param>
    <init-param>
      <param-name>error</param-name>
      <param-value>error</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>com.hwf.servlet.LogoutServlet</servlet-class>
    <init-param>
      <param-name>autoLoginName</param-name>
      <param-value>autoLoginName</param-value>
    </init-param>
    <init-param>
      <param-name>autoLoginSId</param-name>
      <param-value>autoLoginSId</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addProblem</servlet-name>
    <servlet-class>com.hwf.servlet.AddProblemServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addProblem</servlet-name>
    <url-pattern>/addProblem</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>createContest</servlet-name>
    <servlet-class>com.hwf.servlet.CreateContestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>createContest</servlet-name>
    <url-pattern>/createContest</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getTitle</servlet-name>
    <servlet-class>com.hwf.servlet.GetTitleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getTitle</servlet-name>
    <url-pattern>/getTitle</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>submitCode</servlet-name>
    <servlet-class>com.hwf.servlet.SubmitCodeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>submitCode</servlet-name>
    <url-pattern>/submitCode</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>showCode</servlet-name>
    <servlet-class>com.hwf.servlet.ShowCodeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>showCode</servlet-name>
    <url-pattern>/showCode</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>checkPasswd</servlet-name>
    <servlet-class>com.hwf.servlet.CheckPasswdServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>checkPasswd</servlet-name>
    <url-pattern>/checkPasswd</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>editProblem</servlet-name>
    <servlet-class>com.hwf.servlet.EditProblemServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>editProblem</servlet-name>
    <url-pattern>/editProblem</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>showData</servlet-name>
    <servlet-class>com.hwf.servlet.ShowDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>showData</servlet-name>
    <url-pattern>/showData</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>editTestData</servlet-name>
    <servlet-class>com.hwf.servlet.EditTestDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>editTestData</servlet-name>
    <url-pattern>/editTestData</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>updateInfo</servlet-name>
    <servlet-class>com.hwf.servlet.UpdateInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>updateInfo</servlet-name>
    <url-pattern>/updateInfo</url-pattern>
  </servlet-mapping>
</web-app>